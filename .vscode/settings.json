{
  "files.eol": "\n",
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "typescript.tsdk": "node_modules/typescript/lib",
  "json.schemaDownload.enable": false,
  "search.exclude": {
    "**/.git": true,
    "**/node_modules": true,
    "**/tmp": true,
    "*.lock": true,
    ".flutter*": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/node_modules": true,
    "**/*cache": true,
    ".flutter*": true,
    ".packages": true,
    ".metadata": true
  },
  "[dart]": {
    "editor.defaultFormatter": "Dart-Code.dart-code"
  }
}
